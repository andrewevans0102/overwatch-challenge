{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "coverage-report": "ng test --code-coverage --watch=true",
    "prod-test": "ng test --watch=false",
    "prod-build": "ng build --prod",
    "prod-deploy": "firebase deploy",
    "cypress-local": "concurrently \"ng serve\" \"./node_modules/.bin/cypress open\"",
    "cypress-ci": "start-server-and-test \"ng serve\" http-get://localhost:4200 \"./node_modules/.bin/cypress run\"",
    "cypress-install": "npm install cypress --save-dev"
  },
  "private": true,
  "dependencies": {
